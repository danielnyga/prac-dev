# create and train dkb from dbs
praclearn --dbs /home/mareikep/prac_repos/prac/pracmodules/obj_recognition/db/sherlock.db --kb sherlock --module obj_recognition 
praclearn --dbs /home/mareikep/prac_repos/prac/pracmodules/obj_recognition/db/sherlock.db --kb sherlock --module obj_recognition_old 
praclearn --dbs /home/mareikep/prac_repos/prac/pracmodules/obj_recognition/db/new.db,/home/mareikep/prac_repos/prac/pracmodules/obj_recognition/db/sherlock.db --kb new --module obj_recognition

# create manual dkb entry from inference
pracobjrec 'It is a yellow or orange fruit.' -t sherlock orange.n.01 
pracobjrec 'It is a yellow or orange fruit.' -t sherlock orange.n.01 -o

# infer using dkb
pracobjrec -r -m /home/mareikep/prac_repos/prac/pracmodules/obj_recognition/mln/objInf_trained.mln FuzzyLogic 'It is a yellow oval fruit.' 
pracobjrec -r -m /home/mareikep/prac_repos/prac/pracmodules/obj_recognition_old/mln/objInf_trained.mln FuzzyLogic 'It is a yellow oval fruit.' -o

# k-fold cross validation
pracxfold --dbs /home/mareikep/prac_repos/prac/pracmodules/obj_recognition/db/natLan.db --mln /home/mareikep/prac_repos/prac/pracmodules/obj_recognition/mln/objInf.mln --predicate object --domain cluster --folder evalTest --module obj_recognition --folds 10
pracxfold --dbs /home/mareikep/prac_repos/prac/pracmodules/obj_recognition/db/natLan.db --mln /home/mareikep/prac_repos/prac/pracmodules/obj_recognition/mln/objInf.mln --predicate object --domain cluster --folder evalTest_old --module obj_recognition_old --folds 2