# create dkb from db
pracobjrec -d all /home/mareikep/prac_repos/prac/pracmodules/prop_extraction/db/ts_stanford_wn_man.db
pracobjrec -d fruit /home/mareikep/prac_repos/prac/pracmodules/obj_recognition/db/fruit.db
pracobjrec -d kitchenware /home/mareikep/prac_repos/prac/pracmodules/obj_recognition/db/kitchenware.db
pracobjrec -d misc /home/mareikep/prac_repos/prac/pracmodules/obj_recognition/db/misc.db

# create manual dkb entry
pracobjrec -c fruit lemon 'a yellow oval fruit'
pracobjrec -c fruit orange 'an orange round fruit'
pracobjrec -c fruit cherry 'a tiny red round fruit'

# infer using dkb
pracobjrec -k fruit 'a yellow oval fruit'
pracobjrec -k fruit 'an orange round fruit'
pracobjrec -k fruit 'a tiny red round fruit'