# create and train dkb from dbs
praclearn --module obj_recognition --dbs /home/mareikep/prac_repos/prac/pracmodules/obj_recognition/db/sherlockNew.db --kb sherlockNew
praclearn --module obj_recognition --dbs /home/mareikep/prac_repos/prac/pracmodules/obj_recognition/db/sherlockOld.db --kb sherlockOld

# create manual dkb entry from inference
pracobjrec -t sherlockNew orange.n.01 'It is a yellow or orange fruit.'
pracobjrec -t sherlockOld orange.n.01 'It is a yellow or orange fruit.'

# infer using dkb
pracobjrec -r sherlockNew 'It is a yellow oval fruit.'
pracobjrec -r sherlockOld 'It is a yellow oval fruit.'

